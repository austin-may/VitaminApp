type Vitamin {
  VitaminId: ID!
  VitaminType: String!
  Benefits: String!
}

type Query {
  vitamins: [Vitamin!]!,
  vitaminById(vitaminId: Int): Vitamin!
}

input NewVitamin {
  VitaminType: String!
  Benefits: String!
}

input UpdatedVitamin {
  VitaminId: ID!,
  VitaminType: String!,
  Benefits: String!
}

type Mutation {
  createVitamin(input:NewVitamin!): Vitamin,
  updateVitamin(input:UpdatedVitamin!): Vitamin,
  deleteVitamin(vitaminId: Int): Vitamin
}
